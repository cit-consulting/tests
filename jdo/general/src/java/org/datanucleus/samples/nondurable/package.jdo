<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE jdo SYSTEM "file:/javax/jdo/jdo.dtd">
<jdo>
    <package name="org.datanucleus.samples.nondurable">
        <class name="LogEntry" identity-type="nondurable">
            <field name="level"/>
            <field name="message"/>
            <field name="date" persistence-modifier="persistent"/>
        </class>
    </package>
</jdo>